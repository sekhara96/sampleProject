pipeline {
    agent any 
    stages {
        stage('Chekout') {
            steps {
                git branch: 'master', url: 'https://github.com/sekhara96/sampleProject.git'
            }
        }
        stage('Build and Test') {
            steps {
              sh 'ls -ltr'
              // build the project and create a JAR file
              sh 'cd src/main/java/com/sekhar/startproject && mvn clean package'
            }
        }
    }
}
